namespace: "{{ ppeagent_namespace }}"
name: "{{ _ppeagent_deployment_name }}"
replicaCount: "{{ ppeagent_replica_count }}"
port: "{{ ppeagent_port }}"
image:
    name: "{{ _ppeagent_image_name }}"
    tag: "{{ _ppeagent_image_tag }}"
ppeCredentials:
    username: "{{ ppeagent_email }}"
